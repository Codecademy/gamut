{
  "eslint.options": {
    "extensions": [".js", ".jsx", ".md", ".mdx", ".ts", ".tsx"]
  },
  "eslint.probe": [
    "mdx",
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html"
  ],
  "eslint.validate": [
    "mdx",
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html"
  ],
  "files.associations": {
    "*.md": "mdx"
  },
  "[mdx]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120,
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    }
  },
  "search.exclude": {
    "docs": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
