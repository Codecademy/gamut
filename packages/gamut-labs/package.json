{
  "name": "@codecademy/gamut-labs",
  "description": "Brand and experimental component library for Codecademy",
  "version": "28.0.2",
  "author": "Codecademy Engineering <dev@codecademy.com>",
  "sideEffects": [
    "*.css",
    "*.scss",
    "dist/**/[A-Z]**/[A-Z]*.js",
    "dist/**/[A-Z]**/index.js"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:Codecademy/gamut.git"
  },
  "peerDependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "react": ">=16.8.1",
    "react-dom": ">=16.8.1"
  },
  "dependencies": {
    "@codecademy/gamut": "^40.0.0",
    "@codecademy/gamut-icons": "^6.0.0",
    "@codecademy/gamut-illustrations": "^0.20.0",
    "@codecademy/gamut-styles": "^12.0.0",
    "@codecademy/variance": "^0.19.0",
    "classnames": "^2.2.5",
    "framer-motion": "^4.1.17",
    "lodash": "^4.17.5",
    "react-focus-on": "^3.5.1",
    "react-hook-form": "^6.15.5",
    "react-hotkeys-hook": "^2.3.1",
    "react-use": "15.3.8"
  },
  "devDependencies": {
    "@codecademy/gamut-tests": "^3.0.0",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^5.1.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/enzyme": "3.10.7",
    "enzyme": "3.11.0"
  },
  "scripts": {
    "verify": "tsc --noEmit",
    "build:compile": "babel ./src --out-dir ./dist --copy-files --extensions \".ts,.tsx\"",
    "build:clean": "rm -rf dist",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "yarn build:clean && yarn build:compile && yarn build:types",
    "build:watch": "yarn build && onchange ./src -- yarn build:compile && yarn build:types",
    "lernaBuildTask": "yarn build"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
