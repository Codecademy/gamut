  },
  "peerDependencies": {
    "react": ">=16.8.1",
    "react-dom": ">=16.8.1"
  },
  "dependencies": {
    "@codecademy/gamut-styles": "^3.0.0",
    "@types/marked": "^0.6.1",
    "classnames": "^2.2.5",
    "focus-trap-react": "^6.0.0",
