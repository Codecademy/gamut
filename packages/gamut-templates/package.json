{
  "name": "@codecademy/gamut-templates",
  "description": "Templates library for Gamut",
  "version": "2.2.18",
  "author": "Codecademy Engineering <dev@codecademy.com>",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:Codecademy/client-modules.git"
  },
  "peerDependencies": {
    "react": ">=16.8.1",
    "react-dom": ">=16.8.1"
  },
  "dependencies": {
    "@codecademy/gamut": "^2.3.5",
    "@codecademy/gamut-styles": "^2.2.10",
    "classnames": "^2.2.5"
  },
  "scripts": {
    "verify": "tsc --noEmit",
    "build:compile": "babel ./src --out-dir ./dist --copy-files --extensions \".ts,.tsx\"",
    "build:clean": "rm -rf dist",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "yarn build:clean && yarn build:compile && yarn build:types",
    "prepublishOnly": "yarn build"
  },
  "license": "MIT",
  "devDependencies": {
    "copyfiles": "^2.1.1"
  }
}
