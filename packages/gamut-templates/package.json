{
  "name": "@codecademy/gamut-templates",
  "description": "Templates library for Gamut",
  "version": "2.2.11",
  "author": "Codecademy Engineering <dev@codecademy.com>",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:Codecademy/client-modules.git"
  },
  "peerDependencies": {
    "react": ">=16.8.1",
    "react-dom": ">=16.8.1"
  },
  "dependencies": {
    "@codecademy/gamut": "^2.2.10",
    "@codecademy/gamut-styles": "^2.2.10",
    "classnames": "^2.2.5"
  },
  "scripts": {
    "verify": "tsc --noEmit",
    "build:clean": "rm -rf dist lib",
    "build:copy": "copyfiles -u 3 './lib/gamut-templates/src/**/*' ./dist",
    "build": "yarn build:clean && tsc && yarn build:copy"
  },
  "license": "MIT",
  "devDependencies": {
    "copyfiles": "^2.1.1"
  }
}
