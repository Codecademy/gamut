{
  "name": "@codecademy/gamut-icons",
  "description": "Icon library for codecademy.com",
  "version": "6.0.2",
  "author": "Codecademy <dev@codecademy.com>",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "format": "svgo -f ./src/svg --pretty --config=.svgo.yml",
    "build:icons": "rm -rf ./src/icons && svgr --out-dir ./src/icons --typescript ./src/svg",
    "build:types": "tsc --emitDeclarationOnly",
    "build:compile": "babel ./src --out-dir ./dist --copy-files --extensions \".ts,.tsx\"",
    "rename-icons": "find ./src/svg -depth -type f \\( -name '*.svg' ! -name '*-icon.svg' \\) -exec sh -c 'mv \"$1\" \"${1%.svg}-icon.svg\"' _ {} \\;",
    "build": "yarn rename-icons && yarn build:icons && yarn build:compile && yarn build:types",
    "lernaBuildTask": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Codecademy/gamut.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@svgr/cli": "^6.1.2",
    "@types/enzyme": "3.10.7",
    "enzyme": "3.11.0",
    "svgo": "^2.8.0",
    "typescript": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@codecademy/gamut-styles": "^12.1.1",
    "@codecademy/variance": "^0.19.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "lodash": "^4.17.5",
    "react": ">=17.0.2"
  }
}
