{
  "name": "@codecademy/gamut-icons",
  "description": "Icon library for codecademy.com",
  "version": "1.0.0",
  "author": "Codecademy <dev@codecademy.com>",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.5.2"
  },
  "scripts": {
    "format": "svgo -f ./src --pretty --config=svgo.yml",
    "clean": "rm -rf ./dist",
    "build:icons": "svgr -d dist/ts --ext 'tsx' ./src",
    "build:index": "create-index ./dist/ts -r -x tsx js",
    "build:ts": "tsc",
    "clean-index": "mv dist/ts/index.js dist/ts/index.ts; sed -i '' 's/\\.tsx//g' dist/ts/index.ts",
    "build": "yarn build:icons; yarn build:index; yarn clean-index; yarn build:ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:RyzacInc/client-modules.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@svgr/cli": "4.3.2",
    "create-index": "^2.4.0",
    "svgo": "^1.3.0",
    "typescript": "*"
  }
}
