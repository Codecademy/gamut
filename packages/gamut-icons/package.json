{
  "name": "@codecademy/gamut-icons",
  "description": "Icon library for codecademy.com",
  "version": "2.1.1",
  "author": "Codecademy <dev@codecademy.com>",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.5.2"
  },
  "scripts": {
    "format": "svgo -f ./src --pretty --config=.svgo.yml",
    "clean": "rm -rf ./dist; rm -rf ./tmp; mkdir ./tmp",
    "build:icons": "svgr --out-dir ./src/icons --ext 'tsx' ./src/svg",
    "build:types": "tsc --emitDeclarationOnly",
    "build:compile": "babel ./src --out-dir ./dist --copy-files --extensions \".ts,.tsx\"",
    "rename-icons": "find ./src/svg -depth -type f \\( -name '*.svg' ! -name '*-icon.svg' \\) -exec sh -c 'mv \"$1\" \"${1%.svg}-icon.svg\"' _ {} \\;",
    "build": "yarn clean && yarn rename-icons && yarn build:icons && yarn build:compile && yarn build:types",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:RyzacInc/client-modules.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@svgr/cli": "5.0.0",
    "svgo": "^1.3.2",
    "typescript": "*"
  }
}
