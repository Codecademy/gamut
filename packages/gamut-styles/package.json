{
  "name": "@codecademy/gamut-styles",
  "description": "Styleguide & Component library for codecademy.com",
  "version": "8.3.2",
  "author": "Jake Hiller <jake@codecademy.com>",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build:clean": "rm -rf ./dist",
    "build:compile": "tsc",
    "build:transpile": "babel ./src --out-dir ./dist --copy-files --copy-ignored --extensions \".ts,.tsx\"",
    "build": "yarn build:clean && yarn build:compile && yarn build:transpile",
    "lernaBuildTask": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Codecademy/client-modules.git"
  },
  "dependencies": {
    "@codecademy/gamut-system": "^0.5.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
