{
  "name": "@codecademy/gamut-styles",
  "description": "Styleguide & Component library for codecademy.com",
  "version": "10.0.0",
  "author": "Jake Hiller <jake@codecademy.com>",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build:clean": "rm -rf ./dist",
    "build:compile": "tsc",
    "build:transpile": "babel ./src --out-dir ./dist --copy-files --extensions \".ts,.tsx\"",
    "build": "yarn build:clean && yarn build:compile && yarn build:transpile",
    "lernaBuildTask": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Codecademy/client-modules.git"
  },
  "dependencies": {
    "@codecademy/gamut-system": "^0.7.2",
    "@codecademy/variance": "^0.11.2"
  },
  "peerDependencies": {
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.0",
    "component-test-setup": "^0.3.1"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
