{
  "name": "gamut-patterns",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/gamut-patterns/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/src/patterns",
        "{projectRoot}/dist",
        "{projectRoot}/src/patterns"
      ],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/gamut-patterns",
        "commands": [
          "rm -rf ./dist",
          "rm -rf ./src/patterns",
          "svgr --out-dir ./src/patterns --ext 'tsx' ./src/svg",
          "babel ./src --out-dir ./dist --extensions \".ts,.tsx\"",
          "tsc --emitDeclarationOnly",
          "cpy './src/**/*.svg' ./dist"
        ],
        "parallel": false
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/gamut-patterns"],
      "dependsOn": ["build"],
      "options": {
        "jestConfig": "packages/gamut-patterns/jest.config.ts"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "rm -rf ./dist"
      }
    },
    "publish-build": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "commands": [],
        "parallel": false
      }
    }
  }
}
