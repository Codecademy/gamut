{
  "name": "Gamut",
  "private": true,
  "description": "Styleguide & Component libraryy for codecademy.com",
  "version": "1.2.0",
  "license": "Proprietary",
  "author": "Jake Hiller <jake@codecademy.com>",
  "engines": {
    "node": "^8.10.0",
    "yarn": "^1.3.2"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint \"packages/**/*.{js,json}\"",
    "lerna": "lerna",
    "format": "prettier-eslint --prettier-last --write \"./**/*.{js,json,css,scss}\"",
    "build-all": "yarn wsrun build --exclude-missing",
    "start": "yarn wsrun storybook --exclude-missing",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:RyzacInc/gamut.git"
  },
  "workspaces": [
    "packages/*",
    "workspaces/*"
  ],
  "dependencies": {
    "lerna": "^3.0.0-beta.21",
    "wsrun": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "7.2.0",
    "babel-jest": "^20.0.3",
    "babel-polyfill": "^6.23.0",
    "babel-preset-codecademy": "^1.1.0",
    "enzyme": "3.3.0",
    "enzyme-to-json": "3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.3.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "22.1.3",
    "jest-environment-jsdom": "22.1.2",
    "jest-environment-jsdom-global": "1.0.3",
    "lint-staged": "^7.1.0",
    "prettier": "1.12.1",
    "prettier-eslint-cli": "4.7.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.0"
  },
  "lint-staged": {
    "**/*.{js,json,css,scss}": [
      "prettier-eslint --prettier-last --write",
      "git add"
    ]
  }
}
